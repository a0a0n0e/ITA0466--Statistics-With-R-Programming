# Function to find maximum and minimum value of a vector
findMinMax <- function(vector) {
  max_val <- max(vector)
  min_val <- min(vector)
  return(list(maximum = max_val, minimum = min_val))
}

# Test the function
vector <- c(10, 20, 30, 40, 50)
result <- findMinMax(vector)
cat("Maximum value:", result$maximum, "\n")
cat("Minimum value:", result$minimum, "\n")
